                <script>
        function copiarTexto() {
            // Seleciona o campo de entrada
            var pixText = document.getElementById("pixText");

            // Seleciona o texto do campo de entrada
            pixText.select();

            // Copia o texto selecionado para a área de transferência
            document.execCommand("copy");

            // Remove a borda do campo de entrada
            pixText.classList.add("no-border");

            // Modifica o texto e a cor do botão
            var botaoCopiar = document.getElementById("botaoCopiar");
            botaoCopiar.innerHTML = "Código Copiado";
            botaoCopiar.style.color = "#00a650";
          botaoCopiar.style.border = "1px solid #00a650";

        }
    </script>


