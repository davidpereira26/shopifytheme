   <script>
        // Função para compartilhar o link da página atual
        function compartilharLink() {
            const linkDaPagina = window.location.href;
            if (navigator.share) {
                navigator.share({
                    title: document.title,
                    url: linkDaPagina
                })
                .then(() => console.log('Link compartilhado com sucesso'))
                .catch(error => console.error('Erro ao compartilhar link:', error));
            } else {
                // Caso o navegador não suporte a API de compartilhamento, criar um botão de cópia
                const inputLink = document.createElement('input');
                inputLink.value = linkDaPagina;
                document.body.appendChild(inputLink);
                inputLink.select();
                document.execCommand('copy');
                document.body.removeChild(inputLink);
                alert('Link copiado para a área de transferência: ' + linkDaPagina);
            }
        }

        // Adiciona um evento de clique ao botão
        const botaoCompartilhar = document.getElementById('compartilharBtn');
        botaoCompartilhar.addEventListener('click', compartilharLink);
    </script>





