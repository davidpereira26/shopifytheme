<p style="display: none;" >Preço: {{ product.price | money }}</p>


<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Obtém o valor do preço do produto
    var productPrice = {{ product.price | divided_by: 100 }};
    
    // Calcula 35% do preço
    var seguroValue = productPrice * 5;

    // Formata o valor calculado como dinheiro
    var formattedSeguroValue = Shopify.formatMoney(seguroValue, "{{ shop.money_format }}");

    // Exibe o valor formatado no elemento HTML
    var seguroElement = document.getElementById('seguro1');
    seguroElement.textContent = formattedSeguroValue;
  });
</script>



<p style="display: none;">Preço: {{ product.compare_at_price_max | money }}</p>


<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Obtém o valor do preço do produto
    var productPrice = {{ product.compare_at_price_max | divided_by: 100 }};
    
    // Calcula o valor de 1/8 do preço
    var dividedPrice = productPrice / 8;

    // Exibe o valor calculado no elemento HTML
    var dividedPriceElement = document.getElementById('8x');
    dividedPriceElement.textContent = dividedPrice.toFixed(2); // Exibe o valor com 2 casas decimais
  });
</script>


<p style="display: none;" >Preço Normal: {{ product.compare_at_price_max | money }}</p>
<p style="display: none;">Preço de Venda: {{ product.price | money }}</p>


<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Obtém o valor do preço normal e do preço de venda do produto
    var compareAtPrice = {{ product.compare_at_price_max | divided_by: 100 }};
    var salePrice = {{ product.price | divided_by: 100 }};

    // Calcula o desconto em porcentagem
    var discountPercentage = ((compareAtPrice - salePrice) / compareAtPrice) * 100;

    // Exibe o desconto no elemento HTML
    var discountElement = document.getElementById('discount1');
    discountElement.textContent = discountPercentage.toFixed(0); // Exibe apenas a porcentagem sem casas decimais
  });
</script>


