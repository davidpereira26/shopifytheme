<script>
  // Função para verificar a largura da tela
  function isScreenSmall() {
    return window.innerWidth <= 768; // Alterado para 768 pixels
  }

  // Função para verificar a visibilidade de um elemento por ID
  function isElementInViewportById(id) {
    const el = document.getElementById(id);
    if (!el) return false;
    const rect = el.getBoundingClientRect();
    return (
      rect.top >= 0 &&
      rect.left >= 0 &&
      rect.bottom <= (window.innerHeight || document.documentElement.clientHeight)
    );
  }

  // IDs dos elementos que você deseja verificar
  const elementsToCheck = ["in1", "t2", "t3", "t4", "parte1"];

  // Função para mostrar o elemento buyArea quando qualquer um dos elementos estiver visível
  function toggleFloatingButton() {
    const buyArea = document.getElementById("buyArea");
    const isSmallScreen = isScreenSmall();
    let isAnyElementVisible = false; // Variável para rastrear se algum elemento está visível

    if (isSmallScreen) {
      for (const id of elementsToCheck) {
        if (isElementInViewportById(id)) {
          isAnyElementVisible = true; // Defina como true se algum elemento estiver visível
          break; // Sair do loop assim que um dos elementos estiver visível
        }
      }
    }

    // Se a tela for menor ou igual a 768px E algum elemento estiver visível, mostrar o botão
    if (isSmallScreen && isAnyElementVisible) {
      buyArea.style.display = "block";
    } else {
      buyArea.style.display = "none"; // Ocultar o botão para telas maiores que 768px ou nenhum elemento visível
    }
  }

  // Executar a função toggleFloatingButton quando a página for rolada
  window.addEventListener("scroll", toggleFloatingButton);

  // Executar a função toggleFloatingButton quando a janela for redimensionada
  window.addEventListener("resize", toggleFloatingButton);

  // Chame a função toggleFloatingButton uma vez para inicializar o estado correto ao carregar a página
  toggleFloatingButton();
</script>



  

